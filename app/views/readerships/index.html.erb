<h1>Listing read_books</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @read_books.each do |read_book| %>
      <tr>
        <td><%= link_to 'Show', read_book %></td>
        <td><%= link_to 'Edit', edit_read_book_path(read_book) %></td>
        <td><%= link_to 'Destroy', read_book, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Read book', new_read_book_path %>
